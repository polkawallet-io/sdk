{
  "name": "@polkadot/extension-inject",
  "description": "A generic injector (usable to any extension), that populates the base exposed interfaces to be used by dapps.",
  "version": "0.38.8",
  "type": "module",
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "license": "Apache-2",
  "main": "index.js",
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.14.6"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.145",
    "@types/firefox-webext-browser": "^82.0.0"
  },
  "peerDependencies": {
    "@polkadot/api": "*"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./chrome": {
      "require": "./chrome.cjs",
      "default": "./chrome.js"
    },
    "./chrome.d.ts": "./chrome.d.ts",
    "./index.d.ts": "./index.d.ts",
    "./package.json": "./package.json",
    "./packageInfo": {
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    },
    "./packageInfo.d.ts": "./packageInfo.d.ts",
    "./README.md": "./README.md",
    "./types": {
      "require": "./types.cjs",
      "default": "./types.js"
    },
    "./types.d.ts": "./types.d.ts"
  }
}