{
  "name": "@polkadot/extension-dapp",
  "description": "Provides an interfaces around the injected globals for ease of access by dapp developers.",
  "version": "0.38.8",
  "type": "module",
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "license": "Apache-2",
  "main": "index.js",
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@polkadot/extension-inject": "^0.38.8",
    "@polkadot/util": "^6.11.1",
    "@polkadot/util-crypto": "^6.11.1"
  },
  "peerDependencies": {
    "@polkadot/api": "*",
    "@polkadot/util": "*",
    "@polkadot/util-crypto": "*"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./compat": {
      "require": "./compat/index.cjs",
      "default": "./compat/index.js"
    },
    "./compat/index.d.ts": "./compat/index.d.ts",
    "./compat/singleSource": {
      "require": "./compat/singleSource.cjs",
      "default": "./compat/singleSource.js"
    },
    "./compat/singleSource.d.ts": "./compat/singleSource.d.ts",
    "./index.d.ts": "./index.d.ts",
    "./package.json": "./package.json",
    "./packageInfo": {
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    },
    "./packageInfo.d.ts": "./packageInfo.d.ts",
    "./README.md": "./README.md",
    "./util": {
      "require": "./util.cjs",
      "default": "./util.js"
    },
    "./util.d.ts": "./util.d.ts",
    "./wrapBytes": {
      "require": "./wrapBytes.cjs",
      "default": "./wrapBytes.js"
    },
    "./wrapBytes.d.ts": "./wrapBytes.d.ts"
  }
}